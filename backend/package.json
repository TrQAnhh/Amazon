    "migration:profile:create:init": "cross-env npm run typeorm -- migration:create apps/profile/src/db/migrations/init/$npm_config_name",
    "migration:profile:create:dev": "cross-env npm run typeorm -- migration:create apps/profile/src/db/migrations/dev/$npm_config_name",
    "migration:profile:run": "npm run typeorm -- migration:run -d apps/profile/src/config/data-source.ts",
    "migration:profile:revert": "npm run typeorm -- migration:revert -d apps/profile/src/config/data-source.ts",
    "migration:profile:show": "npm run typeorm -- migration:show -d apps/profile/src/config/data-source.ts",
    "migration:profile:drop": "npm run typeorm -- schema:drop -d apps/profile/src/config/data-source.ts",
