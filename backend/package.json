  "scripts": {
    "build": "nest build",
    "build:gateway": "nest build api-gateway",
    "build:identity": "nest build identity",
    "build:product": "nest build product",
    "build:profile": "nest build profile",
    "build:order": "nest build order",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:dev:gateway": "nest start api-gateway --watch",
    "start:dev:identity": "nest start identity --watch",
    "start:dev:product": "nest start product --watch",
    "start:dev:profile": "nest start profile --watch",
    "start:dev:order": "nest start order --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/api-gateway/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/api-gateway/test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "migration:identity:generate:init": "cross-env npm run typeorm -- migration:generate apps/identity/src/db/migrations/init/$npm_config_name -d apps/identity/src/config/data-source.ts --pretty",
    "migration:identity:generate:dev": "cross-env npm run typeorm -- migration:generate apps/identity/src/db/migrations/dev/$npm_config_name -d apps/identity/src/config/data-source.ts --pretty",
    "migration:identity:create:init": "cross-env npm run typeorm -- migration:create apps/identity/src/db/migrations/init/$npm_config_name",
    "migration:identity:create:dev": "cross-env npm run typeorm -- migration:create apps/identity/src/db/migrations/dev/$npm_config_name",
    "migration:identity:run": "npm run typeorm -- migration:run -d apps/identity/src/config/data-source.ts",
    "migration:identity:revert": "npm run typeorm -- migration:revert -d apps/identity/src/config/data-source.ts",
    "migration:identity:show": "npm run typeorm -- migration:show -d apps/identity/src/config/data-source.ts",
    "migration:identity:drop": "npm run typeorm -- schema:drop -d apps/identity/src/config/data-source.ts",
