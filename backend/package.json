    "migration:product:generate:dev": "cross-env npm run typeorm -- migration:generate apps/product/src/db/migrations/dev/$npm_config_name -d apps/product/src/config/data-source.ts --pretty",
    "migration:product:create:init": "cross-env npm run typeorm -- migration:create apps/product/src/db/migrations/init/$npm_config_name",
    "migration:product:create:dev": "cross-env npm run typeorm -- migration:create apps/product/src/db/migrations/dev/$npm_config_name",
    "migration:product:run": "npm run typeorm -- migration:run -d apps/product/src/config/data-source.ts",
    "migration:product:revert": "npm run typeorm -- migration:revert -d apps/product/src/config/data-source.ts",
    "migration:product:show": "npm run typeorm -- migration:show -d apps/product/src/config/data-source.ts",
    "migration:product:drop": "npm run typeorm -- schema:drop -d apps/product/src/config/data-source.ts",
    "migration:order:generate:init": "cross-env npm run typeorm -- migration:generate apps/order/src/db/migrations/init/$npm_config_name -d apps/order/src/config/data-source.ts --pretty",
    "migration:order:generate:dev": "cross-env npm run typeorm -- migration:generate apps/order/src/db/migrations/dev/$npm_config_name -d apps/order/src/config/data-source.ts --pretty",
    "migration:order:create:init": "cross-env npm run typeorm -- migration:create apps/order/src/db/migrations/init/$npm_config_name",
    "migration:order:create:dev": "cross-env npm run typeorm -- migration:create apps/order/src/db/migrations/dev/$npm_config_name",
    "migration:order:run": "npm run typeorm -- migration:run -d apps/order/src/config/data-source.ts",
    "migration:order:revert": "npm run typeorm -- migration:revert -d apps/order/src/config/data-source.ts",
    "migration:order:show": "npm run typeorm -- migration:show -d apps/order/src/config/data-source.ts",
    "migration:order:drop": "npm run typeorm -- schema:drop -d apps/order/src/config/data-source.ts"
  },
